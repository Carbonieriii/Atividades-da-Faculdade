public static void main(String[] args)
{
    Empregado e1 = new Empregado();
    e1.setCodEmpregado(1232);
    e1.setNomeEmpregado("Paulo Silva");
    e1.setCargoEmpregado("Gerente Financeiro");
    e1.setSalarioEmpregado(15400);
    
    Empregado e2 = new Empregado();
    e2.setCodEmpregado(4567);
    e2.setNomeEmpregado("Ana Paula");
    e2.setCargoEmpregado("Analista de Sistema Senior");
    e2.setSalarioEmpregado(10700);
    
    e1.imprimeEmpregados();
    e2.imprimeEmpregados();
}
    
}

///class///
public class Empregado {
    private  int codEmpregado;

    public int getCodEmpregado() {
        return codEmpregado;
    }

    public void setCodEmpregado(int codEmpregado) {
        this.codEmpregado = codEmpregado;
    }

    public String getNomeEmpregado() {
        return nomeEmpregado;
    }

    public void setNomeEmpregado(String nomeEmpregado) {
        this.nomeEmpregado = nomeEmpregado;
    }

    public String getCargoEmpregado() {
        return cargoEmpregado;
    }

    public void setCargoEmpregado(String cargoEmpregado) {
        this.cargoEmpregado = cargoEmpregado;
    }

    public int getSalarioEmpregado() {
        return salarioEmpregado;
    }

    public void setSalarioEmpregado(int salarioEmpregado) {
        this.salarioEmpregado = salarioEmpregado;
    }

    public static int getTotalEmpregado() {
        return totalEmpregado;
    }

    public static void setTotalEmpregado(int totalEmpregado) {
        Empregado.totalEmpregado = totalEmpregado;
    }
    private String nomeEmpregado;
    private String cargoEmpregado;
    private int salarioEmpregado;
    private static int totalEmpregado;
    
    public Empregado(int codEmpregado,String nomeEmpregado,String cargoEmpregado, int salarioEmpregado)
    {
        this.codEmpregado = codEmpregado;
        this.nomeEmpregado = nomeEmpregado;
        this.cargoEmpregado = cargoEmpregado;
        this.salarioEmpregado = salarioEmpregado;
    }
    public Empregado()
    {
        Empregado.totalEmpregado++;
    }
    public void imprimeEmpregados()
    {
        System.out.println("Codigo do empregado: " + codEmpregado);
        System.out.println("Nome do empregado: " + nomeEmpregado);
        System.out.println("Cargo do empregado: " + cargoEmpregado);
        System.out.println("Salario do empregado: " + salarioEmpregado);
        System.out.println("Quantidade de empregado: " + totalEmpregado);
    }
}

Exercicio 7

public class Empregado {
    
    private static int codEmpregado;

    public static int getCodEmpregado() {
        return codEmpregado;
    }

    public static void setCodEmpregado(int codEmpregado) {
        Empregado.codEmpregado = codEmpregado;
    }

    public String getNomeEmpregado() {
        return nomeEmpregado;
    }

    public void setNomeEmpregado(String nomeEmpregado) {
        this.nomeEmpregado = nomeEmpregado;
    }

    public String getCargoEmpregado() {
        return cargoEmpregado;
    }

    public void setCargoEmpregado(String cargoEmpregado) {
        this.cargoEmpregado = cargoEmpregado;
    }

    public int getSalarioEmpregado() {
        return salarioEmpregado;
    }

    public void setSalarioEmpregado(int salarioEmpregado) {
        this.salarioEmpregado = salarioEmpregado;
    }

    public static int getTotalEmpregado() {
        return totalEmpregado;
    }

    public static void setTotalEmpregado(int totalEmpregado) {
        Empregado.totalEmpregado = totalEmpregado;
    }
    private String nomeEmpregado;
    private String cargoEmpregado;
    private int salarioEmpregado;
    private static int totalEmpregado;
    
    public Empregado(int codEmpregado,String nomeEmpregado,String cargoEmpregado, int salarioEmpregado)
    {
        Empregado.codEmpregado = codEmpregado;
        this.nomeEmpregado = nomeEmpregado;
        this.cargoEmpregado = cargoEmpregado;
        this.salarioEmpregado = salarioEmpregado;
    }
    public Empregado()
    {
        Empregado.totalEmpregado++;
    }
    public Empregado(int codEmpregado)
    {
        Empregado.codEmpregado++;
    }
    public void imprimeEmpregados()
    {
        System.out.println("\nCodigo do empregado: " + codEmpregado++);
        System.out.println("Nome do empregado: " + nomeEmpregado);
        System.out.println("Cargo do empregado: " + cargoEmpregado);
        System.out.println("Salario do empregado: " + salarioEmpregado);
        System.out.println("Quantidade de empregado: " + totalEmpregado);
    }
}


LISTA 2 

//main TesteComputador//

package uscs;
 
public class TesteComputador {
    public static void main(String[] args)
    {
 
        Teclado t = new Teclado();
        t.setNumeroSerie("1232");
        t.setFabricante("Logitech");
        t.setCor("Preto");
 
       
       
       
        Monitor mon = new Monitor();
        mon.setNumeroSerie("AFB876");
        mon.setFabricante("Samsung");
        mon.setCor("Preto");
 
   
       
       
        Gabinete g = new Gabinete();
        g.setNumeroSerie("4587HHY");
        g.setFabricante("Logitech");
        g.setCor("Preto");
 
       
       
 
        Mouse m = new Mouse();
        m.setNumeroSerie("HGY6654");
        m.setFabricante("Microsoft");
        m.setCor("Preto");
 
       
        Computador cp = new Computador();
        cp.setNumeroSerie("76TRDE35");
        cp.imprimeComputador();
        System.out.println();
        t.imprimeTeclado();
        System.out.println();
        mon.imprimeMonitor();
        System.out.println();
        g.imprimeGabinete();
        System.out.println();
        m.imprimeMouse();
        System.out.println();
 
    }
}
 

Gabinete

package uscs;
 
public class Gabinete
{
 
       //#region Atributos
       private String numeroSerie;
       private String Fabricante;
       private String Cor;
       //#endregion
   
       //#region Get e Set
       public String getNumeroSerie() {
           return numeroSerie;
       }
       public void setNumeroSerie(String numeroSerie) {
           this.numeroSerie = numeroSerie;
       }
       public String getFabricante() {
           return Fabricante;
       }
       public void setFabricante(String fabricante) {
           this.Fabricante = fabricante;
       }
       public String getCor() {
           return Cor;
       }
       public void setCor(String cor) {
           this.Cor = cor;
       }
       //#endregion
   
       //#region Construtores
       public Gabinete(String numeroSerie, String Fabricante, String Cor)
       {
           this.numeroSerie = numeroSerie;
           this.Fabricante = Fabricante;
           this.Cor = Cor;
       }
   
       public Gabinete()
       {
   
       }
       //#endregion
   
       //#region Fun√ß√£o
       public void imprimeGabinete()
       {
           System.out.println("O numero de serie do gabinete √©: " + numeroSerie);
           System.out.println("O fabricante do gabinete √©: " + Fabricante);
           System.out.println("A cor do gabinete √©: " + Cor);
       }
       //#endregion
     

Teclado
package uscs;
 
public class Teclado
{
   
    //#region Atributos
    private String numeroSerie;
    private String Fabricante;
    private String Cor;
    //#endregion
 
    //#region Get e Set
    public String getNumeroSerie() {
        return numeroSerie;
    }
    public void setNumeroSerie(String numeroSerie) {
        this.numeroSerie = numeroSerie;
    }
    public String getFabricante() {
        return Fabricante;
    }
    public void setFabricante(String fabricante) {
        this.Fabricante = fabricante;
    }
    public String getCor() {
        return Cor;
    }
    public void setCor(String cor) {
        this.Cor = cor;
    }
    //#endregion
 
    //#region Construtores
    public Teclado(String numeroSerie, String Fabricante, String Cor)
    {
        this.numeroSerie = numeroSerie;
        this.Fabricante = Fabricante;
        this.Cor = Cor;
    }
 
    public Teclado()
    {
 
    }
    //#endregion
 
    //#region Fun√ß√£o
    public void imprimeTeclado()
    {
        System.out.println("O numero de serie do teclado √©: " + numeroSerie);
        System.out.println("O fabricante do teclado √©: " + Fabricante);
        System.out.println("A cor do teclado √©: " + Cor);
    }
    //#endregion
 
}
 

Mouse

package uscs;
 
public class Mouse
{
 
   
    //#region Atributos
    private String numeroSerie;
    private String Fabricante;
    private String Cor;
    //#endregion
 
    //#region Get e Set
    public String getNumeroSerie() {
        return numeroSerie;
    }
    public void setNumeroSerie(String numeroSerie) {
        this.numeroSerie = numeroSerie;
    }
    public String getFabricante() {
        return Fabricante;
    }
    public void setFabricante(String fabricante) {
        this.Fabricante = fabricante;
    }
    public String getCor() {
        return Cor;
    }
    public void setCor(String cor) {
        this.Cor = cor;
    }
    //#endregion
 
    //#region Construtores
    public Mouse(String numeroSerie, String Fabricante, String Cor)
    {
        this.numeroSerie = numeroSerie;
        this.Fabricante = Fabricante;
        this.Cor = Cor;
    }
 
    public Mouse()
    {
 
    }
    //#endregion
 
    //#region Fun√ß√£o
    public void imprimeMouse()
    {
        System.out.println("O numero de serie do mouse √©: " + numeroSerie);
        System.out.println("O fabricante do mouse √©: " + Fabricante);
        System.out.println("A cor do mouse √©: " + Cor);
    }
    //#endregion
   
}


