///class///

public class Empregado {
    public  int codEmpregado;
    public String nomeEmpregado;
    public String cargoEmpregado;
    public int salarioEmpregado;
    public static int totalEmpregado;
    
    public Empregado(int codEmpregado,String nomeEmpregado,String cargoEmpregado, int salarioEmpregado)
    {
        this.codEmpregado = codEmpregado;
        this.nomeEmpregado = nomeEmpregado;
        this.cargoEmpregado = cargoEmpregado;
        this.salarioEmpregado = salarioEmpregado;
    }
    public Empregado()
    {
        Empregado.totalEmpregado++;
    }
    public void imprimeEmpregados()
    {
        System.out.println("Codigo do empregado: " + codEmpregado);
        System.out.println("Nome do empregado: " + nomeEmpregado);
        System.out.println("Cargo do empregado: " + cargoEmpregado);
        System.out.println("Salario do empregado: " + salarioEmpregado);
        System.out.println("Quantidade de empregado: " + totalEmpregado);
    }
}

///psvm///

public class TesteEmpregado {
    
public static void main(String[] args)
{
    Empregado e1 = new Empregado();
    e1.codEmpregado = (1232);
    e1.nomeEmpregado = ("Paulo Silva");
    e1.cargoEmpregado = ("Gerente financeiro");
    e1.salarioEmpregado = (15400);
    
    Empregado e2 = new Empregado();
    e2.codEmpregado = (4567);
    e2.nomeEmpregado = ("Ana Paula");
    e2.cargoEmpregado = ("Analista de Sistema Senior");
    e2.salarioEmpregado = (10700);
    
    e1.imprimeEmpregados();
    e2.imprimeEmpregados();
}
    
}

