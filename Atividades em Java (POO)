Computador

package uscs;
 
public class Computador
{
 
    //#region Atributos
    private String numeroSerie;
    private String Teclado;
    private String Mouse;
    private String Gabinete;
    //#endregion
 
    //#region Get e Set
    public String getNumeroSerie() {
        return numeroSerie;
    }
    public void setNumeroSerie(String numeroSerie) {
        this.numeroSerie = numeroSerie;
    }
    public String getTeclado() {
        return Teclado;
    }
    public void setTeclado(String teclado) {
        this.Teclado = teclado;
    }
    public String getMouse() {
        return Mouse;
    }
    public void setMouse(String mouse) {
        this.Mouse = mouse;
    }
    public String getGabinete() {
        return Gabinete;
    }
    public void setGabinete(String gabinete) {
        this.Gabinete = gabinete;
    }
    //#endregion
 
    //#region Construtores
    public Computador(String numeroSerie)
    {
        this.numeroSerie = numeroSerie;
    }
 
    public Computador()
    {
 
    }
    //#endregion
 
    //#region Fun√ß√£o
    public void imprimeComputador()
    {
        System.out.println("O numero de serie do computador √©: " + numeroSerie);
    }
   
   LISTA 3 EM DESENVOLVIMENTO
   
   Exercicio 1 

//class Empregado//

package uscs;
 
public class Empregado
{
 
    //#region Atributos
    private String nome;
    private String endereco;
    private int valorHora;
    private int qtdHora;
    //#endregion
 
    //#region Get Set
    public String getNome() {
        return nome;
    }
    public void setNome(String nome) {
        this.nome = nome;
    }
    public String getEndereco() {
        return endereco;
    }
    public void setEndereco(String endereco) {
        this.endereco = endereco;
    }
    public int getValorHora() {
        return valorHora;
    }
    public void setValorHora(int valorHora) {
        this.valorHora = valorHora;
    }
    public int getQtdHora() {
        return qtdHora;
    }
    public void setQtdHora(int qtdHora) {
        this.qtdHora = qtdHora;
    }
    //#endregion
 
    //#region Construtores
    public Empregado(String nome, String endereco, int valorHora, int qtdHora)
    {
       this.nome = nome;
       this.endereco = endereco;
       this.valorHora = valorHora;
       this.qtdHora = qtdHora;
    }
 
    public Empregado()
    {
 
    }
    //#endregion
 
    //#region Metodos
 
    public void imprimeEmpregado()
    {
        System.out.println("Nome: " + nome);
        System.out.println("Endere√ßo: " + endereco);
        System.out.println("Salario: " + valorHora);
        System.out.println("Quantidade de hoeras trabalhadas: " + qtdHora);
    }
   

